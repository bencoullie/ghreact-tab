{
  "short_name": "Ghreact",
  "name": "React Extension",
  "description": "A simple chrome addon to see how many code reviews a person has been assigned to.",
  "manifest_version": 2,
  "icons": {
    "16": "images/green-gh-16.png",
    "48": "images/green-gh-48.png",
    "128": "images/green-gh-128.png"
  },
  "browser_action": {
    "default_icon": "images/green-gh-128.png",
    "default_title": "React Ext"
  },
  "version": "1.0",
  "permissions": [
    "management",
    "tabs"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
